{
  "name": "alma-backend",
  "version": "1.0.0",
  "description": "ALMA is a user manage system for the ANEIAP association to organize the information from his associates and manage their internal processes.",
  "main": "./dist/index.js",
  "scripts": {
    "build:dev": "node -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "build:prod": "rm -rf dist/* && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "nodemon",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "node dist",
    "test": "jest --coverage --collectCoverageFrom=src/**",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergio-pomarico/alma-backend.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "clean architecture",
    "tdd"
  ],
  "author": {
    "name": "Sergio Pom√°rico",
    "email": "sergiodavid21@gmail.com",
    "url": "https://sergiopomarico.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergio-pomarico/alma-backend/issues"
  },
  "homepage": "https://github.com/sergio-pomarico/alma-backend#readme",
  "dependencies": {
    "awilix": "^4.2.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.4",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
